{
	"name": "11ty-lakeback",
	"version": "1.0.0",
	"description": "11ty lakeback by Adam Dj Brett",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"start": "npx @11ty/eleventy --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/adamdjbrett/lakeback.org.git"
	},
	"author": {
		"name": "Adam Dj Brett",
		"email": "info@adamdjbrett.com",
		"url": "https://adamdjbrett.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"devDependencies": {
		"@11ty/eleventy": "3.1.2",
		"@11ty/eleventy-img": "6.0.4",
		"@11ty/eleventy-navigation": "1.0.5",
		"@11ty/eleventy-plugin-rss": "2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "5.0.2",
		"luxon": "3.7.2",
		"markdown-it": "14.1.1",
		"zod": "4.3.6",
		"zod-validation-error": "5.0.0"
	},
	"dependencies": {
		"@11ty/eleventy-upgrade-help": "3.0.2",
		"@zachleat/heading-anchors": "1.0.5",
		"clean-css": "5.3.3",
		"eleventy-plugin-toc": "1.1.5",
		"markdown-it-anchor": "9.2.0",
		"string-strip-html": "13.5.3"
	}
}
