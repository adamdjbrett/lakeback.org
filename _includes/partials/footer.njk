<footer class="container-fluid border-top border-bottom">
<div class="row mt-3">
<div class="col-md-6 p-3 p-md-5" aria-label="footer title">
<h3 class="display-6"><a href="{{metadata.url}}">{{metadata.footer.title}}</a></h3>
</div>
<div class="col-md-6 p-3 p-md-5" aria-label="footer menu">
<ul class="row">{% for ff in metadata.footer.item %}
<li class="col-md-4 col-4"><a class="non" href="{{ff.url}}">{{ff.nav}}</a></li>{% endfor %}
</ul>
</div>
</div>
</footer>
<p class="p-1 text-center small bg-body-tertiary" aria-label="copyrights">
Built with <a href="https://11ty.dev">{{ eleventy.generator }}</a> {{metadata.copyrights.text1}} <a href="{{metadata.copyrights.text2_url}}">{{metadata.copyrights.text2}}</a>
</p>
{%- js %}{% include "public/js/bs.js" %}{% endjs %}<script type="module" src="{% getBundleFileUrl "js" %}"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
<script>
  if (window.netlifyIdentity) {
    window.netlifyIdentity.on("init", (user) => {
      if (!user) {
        window.netlifyIdentity.on("login", () => {
          document.location.href = "/admin/";
        });
      }
    });
  }
</script>
